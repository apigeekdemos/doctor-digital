<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<XMLThreatProtection async="false" continueOnError="false" enabled="true" name="HPX_CheckXMLThreat">
    <DisplayName>CheckXMLThreat</DisplayName>
    <FaultRules/>
    <Properties/>
    <NameLimits>
        <Element>30</Element>
        <Attribute>10</Attribute>
        <NamespacePrefix>20</NamespacePrefix>
        <ProcessingInstructionTarget>-1</ProcessingInstructionTarget>
    </NameLimits>
    <Source>request</Source>
    <StructureLimits>
        <NodeDepth>10</NodeDepth>
        <AttributeCountPerElement>3</AttributeCountPerElement>
        <NamespaceCountPerElement>3</NamespaceCountPerElement>
        <ChildCount includeText="true" includeComment="false" includeProcessingInstruction="true" includeElement="true">30</ChildCount>
    </StructureLimits>
    <ValueLimits>
        <Text>-1</Text>
        <Attribute>-1</Attribute>
        <NamespaceURI>50</NamespaceURI>
        <Comment>-1</Comment>
        <ProcessingInstructionData>-1</ProcessingInstructionData>
    </ValueLimits>
</XMLThreatProtection>
